<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ManagerEntity" table="Manager" schema="dbo" catalog="OnlineTest">
        <id name="mgrId">
            <column name="MgrID" sql-type="int"/>
            <generator class="native"/>
        </id>
        <property name="mgrName">
            <column name="MgrName" sql-type="nvarchar(20)" length="20"/>
        </property>
        <property name="mgrPwd">
            <column name="MgrPwd" sql-type="nvarchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>