<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ScEntity" table="SC" schema="dbo" catalog="OnlineTest">
        <id name="scid">
            <column name="SCID" sql-type="int"/>
            <generator class="native"/>
        </id>
        <property name="stuId">
            <column name="StuID" sql-type="int"/>
        </property>
        <property name="cCourseId">
            <column name="CCourseID" sql-type="int"/>
        </property>
        <property name="cCourseState">
            <column name="CCourseState" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>