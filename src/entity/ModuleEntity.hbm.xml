<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ModuleEntity" table="Module" schema="dbo" catalog="OnlineTest">
        <id name="moduleId">
            <column name="ModuleID" sql-type="int"/>
            <generator class="native"/>
        </id>
        <property name="moduleName">
            <column name="ModuleName" sql-type="nvarchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>