<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.PaperEntity" table="Paper" schema="dbo" catalog="OnlineTest">
        <id name="pid">
            <column name="PID" sql-type="int"/>
            <generator class="native"/>
        </id>
        <property name="pname">
            <column name="Pname" sql-type="nvarchar(20)" length="20"/>
        </property>
        <property name="cCourseId">
            <column name="CCourseID" sql-type="int"/>
        </property>
        <property name="mgrId">
            <column name="MgrID" sql-type="int" not-null="true"/>
        </property>
        <property name="modifyTime">
            <column name="ModifyTime" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>